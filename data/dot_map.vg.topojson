{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 300,
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/rui0007/FIT3179-DataVis_2/main/data/nsw_localities.topojson",
          "format": {
            "type": "topojson",
            "feature": "STATE"
          }
        },
        "projection": {
          "type": "equalEarth"
        },
        "mark": {
          "type": "geoshape",
          "fill": "blue",
          "stroke": "white"
        }
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/rui0007/FIT3179-DataVis_2/main/data/dotmap_vg.csv"
        },
        "projection": {
          "type": "equalEarth"
        },
        "mark": "circle",
        "encoding": {
          "longitude": {
            "field": "Longitude",
            "type": "quantitative"
          },
          "latitude": {
            "field": "Latitude",
            "type": "quantitative"
          },
          "size": {"value": 5},
          "color": {"value": "steelblue"}
        }
      }
    ]
  }
  