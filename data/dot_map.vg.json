{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": "container",
    "height": 500,
    "params": [

    ],
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/rui0007/FIT3179-DataVis_2/main/js/nsw_localities_shp.json",
          "format": {
            "type": "topojson",
            "feature": "arcs"
          }
        },
        "projection": {
          "type": "mercator"
        },
        "mark": {
          "type": "geoshape",
          "fill": "white",
          "stroke": "white"
        }
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/rui0007/FIT3179-DataVis_2/main/data/confrimed_location_clean.csv"
        },
        "projection": {
          "type": "mercator"
        },
        "mark": "circle",
        "encoding": {
          "longitude": {
            "field": "Long",
            "type": "quantitative"
          },
          "latitude": {
            "field": "Lat",
            "type": "quantitative"
          },
          "size": {"value": 5},
          "color": {"value": "steelblue"}
        }
      }
    ]
  }
  