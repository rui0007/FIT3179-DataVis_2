{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width":700,
    "height":300,
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/rui0007/FIT3179-DataVis_2/main/js/state_level.json",
          "format":{
            "type": "topojson",
            "feature": "state_level"
          }
        },
        "projection": {
          "type":"mercator"
        },
        "mark":{
          "type": "geoshape",
          "fill":"white",
          "fillOpacity":0.2,
          "stroke":"white"
        }
      },
      {
        "data":{
          "url": "https://raw.githubusercontent.com/rui0007/FIT3179-DataVis_2/main/data/clinic_small.csv"
        },
        "projection": {
          "type":"mercator"
        },
        "mark":"point",
        "encoding": {
          "longitude":{
            "field":"Longitude",
            "type": "quantitative"
          },
          "latitude":{
            "field":"Latitude",
            "type": "quantitative"
          },
          "size":{"value":10},
          "color":{"value":"streeblue"}
        }
      }
    ]
  }