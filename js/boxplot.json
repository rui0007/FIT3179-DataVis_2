{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A vertical box plot showing median and lower and upper quartiles of the distribution of body mass of penguins.",
  "width":500,
  "height":400,
  "data": { 
    "url": "https://raw.githubusercontent.com/rui0007/FIT3179-DataVis_2/main/data/box.csv"
  },

  "transform": [
    {
      "fold": [
        "JAN",
        "FEB",
        "MAR",
        "APR",
        "MAY",
        "JUN",
        "JUL",
        "AUG",
        "SEP",
        "OCT"
      ]
    }
  ],
  "mark": "boxplot",
  "encoding": {
    "y": { "field": "State", "type": "nominal" },
    "color": { "field": "State", "type": "nominal", "legend": null },
    "x": {
      "field": "value",
      "type": "quantitative",
      "scale": { "zero": false }
    }
  }
}
